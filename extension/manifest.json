{
  "manifest_version": 2,
  "name": "Quoted",
  "version": "0.1",
  "permissions": [
  "https://quotedserver.herokuapp.com/*"
  ],
  "icons": { "16": "icon16.png",
  "32": "icon32.png",
  "128": "icon128.png" },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "css": ["tooltip.css", "opentip.css"],
    "js": ["jquery-2.2.2.min.js", "opentip-jquery.js", "content.js"]
    }],
    "browser_action": {
      "default_title": "Quoted",
      "default_icon": "icon16.png"
    }
  }
